{
  "name": "api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/api",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx-go/nx-go:build",
      "options": {
        "main": "{projectRoot}/main.go"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/api",
        "commands": ["air"]
      }
    },
    "serve": {
      "executor": "@nx-go/nx-go:serve",
      "options": {
        "main": "{projectRoot}/main.go"
      }
    },
    "test": {
      "executor": "@nx-go/nx-go:test"
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    },
    "tidy": {
      "executor": "@nx-go/nx-go:tidy"
    },
    "create-migration": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/api",
        "commands": ["make create_migration"]
      }
    },
    "migrate-up": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/api",
        "commands": ["make migrate_up"]
      }
    },
    "migrate-down": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/api",
        "commands": ["make migrate_down"]
      }
    }
  }
}
